{
  "name": "mojing",
  "version": "1.0.0",
  "private": true,
  "author": {
    "name": "Fusang Brain",
    "email": "joson.chou@outlook.com"
  },
  "main": "./dist/main/main.js",
  "description": "An out-of-box UI solution for enterprise applications",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "deploy": "cross-env ANT_DESIGN_PRO_ONLY_DO_NOT_USE_IN_YOUR_PRODUCTION=site npm run site && npm run gh-pages",
    "format-imports": "import-sort --write '**/*.{js,jsx,ts,tsx}'",
    "gh-pages": "cp CNAME ./dist/ && gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "check-prettier lint",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"**/*\"",
    "start": "concurrently \"npm run start:main\" \"npm run start:renderer\"",
    "start:renderer": "cross-env APP_ROOT=src/renderer PORT=8520 BROWSER=none umi dev",
    "start:renderer:no-mock": "cross-env APP_ROOT=src/renderer BROWSER=none MOCK=none umi dev",
    "start:main": "electron-webpack dev",
    "build": "npm run build:renderer && npm run build:main",
    "build:renderer": "cross-env APP_ROOT=src/renderer umi build",
    "build:main": "cross-env NODE_ENV=production webpack --config ./build/webpack/main.config.js",
    "pack": "npm run build && electron-builder --dir",
    "dist": "electron-builder",
    "dist:mac": "electron-builder -m",
    "dist:win": "electron-builder -w",
    "dist:linux": "electron-builder -l",
    "preapostinstall": "node tools/copy-app-dep.js",
    "postinstall": "electron-builder install-app-deps",
    "test": "umi test",
    "tsc:main": "tsc -p src/main",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "umi test ./src/components"
  },
  "electronWebpack": {
    "main": {
      "sourceDirectory": "src/main",
      "webpackConfig": "webpack.main.transformer.js"
    },
    "renderer": {
      "sourceDirectory": null
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "about-window": "^1.13.0",
    "axios": "^0.19.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-is": "^3.0.0",
    "electron-log": "^3.0.5",
    "electron-store": "^3.2.0",
    "electron-updater": "^4.0.6",
    "global": "^4.4.0",
    "path": "^0.12.7",
    "path-to-regexp": "^3.0.0",
    "qs": "^6.7.0",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@ant-design/colors": "^3.1.0",
    "@ant-design/dark-theme": "^0.2.2",
    "@ant-design/pro-cli": "^1.0.0",
    "@ant-design/pro-layout": "^4.5.9",
    "@antv/data-set": "^0.10.2",
    "@babel/runtime": "^7.5.5",
    "@devexpress/dx-react-core": "^2.0.4",
    "@devexpress/dx-react-grid": "^2.0.4",
    "@devexpress/dx-react-grid-bootstrap3": "^2.0.4",
    "@devexpress/dx-react-grid-bootstrap4": "^2.0.4",
    "@types/classnames": "^2.2.7",
    "@types/dot-object": "^1.7.0",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/express": "^4.17.0",
    "@types/history": "^4.7.2",
    "@types/jest": "^24.0.13",
    "@types/lodash": "^4.14.133",
    "@types/qs": "^6.5.3",
    "@types/react": "^16.8.19",
    "@types/react-document-title": "^2.0.3",
    "@types/react-dom": "^16.8.4",
    "@types/react-virtualized-auto-sizer": "^1.0.0",
    "@umijs/fabric": "^1.1.0",
    "antd": "^3.20.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-plugin-import": "^1.12.1",
    "chalk": "^2.4.2",
    "check-prettier": "^1.0.3",
    "classnames": "^2.2.6",
    "concurrently": "^4.1.2",
    "cross-env": "^5.2.0",
    "cross-port-killer": "^1.1.1",
    "dot-object": "^1.9.0",
    "dva": "^2.4.1",
    "electron": "^5.0.1",
    "electron-builder": "^20.40.2",
    "electron-debug": "^3.0.0",
    "electron-rebuild": "^1.8.4",
    "electron-webpack": "^2.7.4",
    "electron-webpack-ts": "^3.2.0",
    "enzyme": "^3.9.0",
    "eslint": "^5.16.0",
    "express": "^4.17.1",
    "gh-pages": "^2.0.1",
    "hash.js": "^1.1.7",
    "husky": "^3.0.0",
    "import-sort-cli": "^6.0.0",
    "import-sort-parser-babylon": "^6.0.0",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style-module": "^6.0.0",
    "jest-puppeteer": "^4.2.0",
    "lint-staged": "^9.0.0",
    "lodash": "^4.17.11",
    "mockjs": "^1.0.1-beta3",
    "moment": "^2.24.0",
    "node-fetch": "^2.6.0",
    "omit.js": "^1.0.2",
    "path-to-regexp": "^3.0.0",
    "prettier": "^1.17.1",
    "pro-download": "1.0.1",
    "qs": "^6.7.0",
    "react": "^16.8.6",
    "react-bootstrap": "0.32.4",
    "react-copy-to-clipboard": "^5.0.1",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.8.6",
    "react-virtualized-auto-sizer": "^1.0.2",
    "redux": "^4.0.1",
    "roadhog": "2.5.0-beta.1",
    "slash2": "^2.0.0",
    "source-map-support": "^0.5.13",
    "stylelint": "^10.1.0",
    "ts-loader": "^6.0.4",
    "typescript": "3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "umi": "^2.8.7",
    "umi-plugin-ga": "^1.1.3",
    "umi-plugin-pro": "^1.0.2",
    "umi-plugin-pro-block": "^1.3.2",
    "umi-plugin-react": "^1.9.5",
    "umi-request": "^1.0.8",
    "umi-types": "^0.3.8",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7",
    "webpack-merge": "^4.2.2",
    "webpack-theme-color-replacer": "^1.2.15"
  },
  "optionalDependencies": {
    "puppeteer": "^1.17.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "build": {
    "productName": "MoJingDianJia",
    "files": [
      "./dist/**/*",
      "./pages/**/*",
      "./node_modules/**/*"
    ],
    "appId": "io.meyup.eyeware-store",
    "asar": false,
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "nsis": {
      "oneClick": false,
      "createDesktopShortcut": true,
      "menuCategory": false,
      "perMachine": true,
      "license": "LICENSE",
      "deleteAppDataOnUninstall": true,
      "allowToChangeInstallationDirectory": true
    },
    "mac": {
      "gatekeeperAssess": false,
      "darkModeSupport": true
    },
    "dmg": {},
    "publish": [
      {
        "provider": "generic",
        "url": "http://release.meyup.io/store"
      }
    ],
    "copyright": "Copyright Â© 2019 ${author}"
  },
  "checkFiles": [
    "src/**/*.js*",
    "src/**/*.ts*",
    "src/**/*.less",
    "config/**/*.js*",
    "scripts/**/*.js"
  ]
}
